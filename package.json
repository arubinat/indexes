{
  "name": "ghostgirl",
  "version": "1.0.0",
  "description": "portal about a fictional character, called Charlotte Usher",
  "browserslist": "> 2%, last 2 versions, not dead",
  "source": "src/index.html",
  "scripts": {
    "start": "npm-run-all clean parcel:dev",
    "build": "npm-run-all clean parcel:build",
    "dev": "parcel src/index.html",
    "pro": "parcel build src/index.html",
    "parcel:dev": "parcel src/index.html --open",
    "parcel:build": "parcel build",
    "clean": "rimraf dist .cache .cache-loader .parcel-cache",
    "watch": "parcel watch src/index.html"
  },
  "keywords": [
    "ghostgirl",
    "charlotte",
    "usher",
    "tonya",
    "books",
    "character",
    "fiction"
  ],
  "author": "arubinat",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "autoprefixer": "^10.4.4",
    "npm-run-all": "^4.1.5",
    "parcel": "latest",
    "postcss": "^8.4.12",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@parcel/transformer-babel": "^2.4.1",
    "font-awesome": "^4.7.0"
  }
}
